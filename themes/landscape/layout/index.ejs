<article class="article article-entry article-type-<%= page.layout %>" itemscope itemprop="articleBody">
  <%- page.content %>
</article>

<% if (site.posts.length){ %>
  <h2 class="category-header"><%= __('Articles') %></h2>
  <ul>
    <% site.posts.sort('date', -1).each(function(post){ %>
      <a class="archive-article-link" href="<%- url_for(post.path) %>">
        <li class="archive-article-inner archive-article-header">
          <h1 class="archive-article-title" itemprop="name">
            <%= post.title || __('no_title') %>
          </h1>
          <time class="archive-article-date" datetime="<%= date_xml(post.date) %>" itemprop="datePublished"><%= date(post.date, config.date_format) %></time>
        </li>
      </a>
    <% }) %>
  </ul>
<% } %>
